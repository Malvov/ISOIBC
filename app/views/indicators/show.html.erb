<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @indicator.name %>
</p>
<table>
  <thead>
    <tr>
      <td class="h2">Puntos a evaluar</td>
    </tr>
  </thead>
  <tbody>
    <% @indicator.parts.each do |part| %>
      <tr>
        <td><%= part.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'Edit', edit_indicator_path(@indicator) %> |
<%= link_to 'Back', indicators_path %>
