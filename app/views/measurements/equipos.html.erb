<h1>Equipos industriales</h1>
<%= link_to 'Nueva', new_measurement_path, class: 'btn btn-success' %>

<table class="table">
  <thead>
    <tr>
      <th>Nombre</th>
    </tr>
  </thead>
  <tbody>
    <% @equipment.each do |equipment| %>
      <tr>
        <td><%= equipment.name %></td>
        <td><%= link_to 'Ver mediciones', mediciones_path(equipment) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>